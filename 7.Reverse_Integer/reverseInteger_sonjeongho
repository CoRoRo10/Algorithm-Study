var reverse = function(x) {
    let max = (2**31)-1;
    let min = - (2**31);
    
     if(x>0) {
         x = parseInt((String(x).split("").reverse().join("")));
     } else {
        x = -parseInt((String(x).split("").reverse().join("")));
     }
    
    if(x >= max || x <= min) x = 0;
    
     return x;
};
